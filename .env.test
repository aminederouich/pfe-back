# Variables d'environnement pour les tests
# Ces variables sont utilisées uniquement pendant l'exécution des tests

# Mode test
NODE_ENV=test

# Configuration de test pour éviter les connexions réelles
TEST_MODE=true

# Configuration Firebase pour les tests (mocks)
FIREBASE_PROJECT_ID=test-project
FIREBASE_PRIVATE_KEY=""
FIREBASE_CLIENT_EMAIL=""

# Configuration de base pour les tests
PORT=3001
API_BASE_URL=http://localhost:3001

# Désactiver les logs en mode test
LOG_LEVEL=error

# Configuration Jest
JEST_TIMEOUT=15000
